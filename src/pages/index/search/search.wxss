/* 星级样式 */
.start {
  position: relative;
  overflow: hidden;
  border-radius: 5rpx;
  flex-shrink: 0;
  display: flex;
  align-items: center;
  width: 140rpx;
  height: 26rpx;
}
.gray {
  background: #dcdcdc;
}
.abOrange {
  position: absolute;
  top: 0;
  left: 0;
  width: 0;
  height: 26rpx;
  border-radius: 5rpx;
  background: #ff471e;
}
.shop-score {
  width: 140rpx;
  height: 26rpx;
  z-index: 1; 
}

.img-logo{
  width: 150rpx;
  height: 150rpx;
  background: #eee;
  margin-right: 20rpx;
}
.shop-name{
  font-size: 28rpx;
  color: #1b1b1b;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  line-height: 35rpx;
  margin-bottom: 15rpx;
}
.shop-fun{
  display: flex;
  align-items: center;
}
.cat_name,.addressIndex{
  font-size: 24rpx;
  color: #1b1b1b;
  margin-left: 20rpx;
}
.bear{
  margin-top: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.perCost{
  font-size: 28rpx;
  color: #ff6c00;
}
.distance{
  font-size: 26rpx;
  color: #1b1b1b;
}